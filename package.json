{
  "name": "karavan-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "export": "next build && next export",
    "deploy": "npm run build",
    "analyze": "ANALYZE=true npm run build",
    "clean": "rm -rf .next out node_modules/.cache .turbo",
    "clean:all": "rm -rf .next out node_modules/.cache .turbo node_modules package-lock.json",
    "clean:install": "npm run clean:all && npm install",
    "fresh": "npm run clean && npm run dev",
    "restart": "pkill -f 'next dev' || true && npm run clean && npm run dev",
    "kill:ports": "lsof -ti:3000,3001,3002,3003 | xargs kill -9 || true"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@heroicons/react": "2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "clsx": "^2.1.1",
    "d3-geo": "^3.1.1",
    "flag-icons": "7.5.0",
    "framer-motion": "^11.18.2",
    "next": "15.5.4",
    "next-intl": "^4.3.12",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.53.0",
    "react-simple-maps": "^3.0.0",
    "sharp": "0.34.4",
    "tailwind-merge": "^2.6.0",
    "topojson-client": "^3.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/topojson-client": "^3.1.5",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  },
  "overrides": {
    "react-simple-maps": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
